<?xml version="1.0" ?>
<sdf version="1.8">
  <world name="cyberbrick_world">
    
    <!-- Physics -->
    <physics name="1ms" type="ignored">
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1.0</real_time_factor>
    </physics>

    <!-- Required plugins for Ignition Gazebo -->
    <plugin filename="ignition-gazebo-physics-system" name="ignition::gazebo::systems::Physics"/>
    <plugin filename="ignition-gazebo-user-commands-system" name="ignition::gazebo::systems::UserCommands"/>
    <plugin filename="ignition-gazebo-scene-broadcaster-system" name="ignition::gazebo::systems::SceneBroadcaster"/>
    <plugin filename="ignition-gazebo-sensors-system" name="ignition::gazebo::systems::Sensors">
      <render_engine>ogre2</render_engine>
    </plugin>

    <!-- Lighting -->
    <light type="directional" name="sun">
      <cast_shadows>true</cast_shadows>
      <pose>0 0 10 0 0 0</pose>
      <diffuse>0.8 0.8 0.8 1</diffuse>
      <specular>0.2 0.2 0.2 1</specular>
      <attenuation>
        <range>1000</range>
        <constant>0.9</constant>
        <linear>0.01</linear>
        <quadratic>0.001</quadratic>
      </attenuation>
      <direction>-0.5 0.1 -0.9</direction>
    </light>

    <!-- Ground plane -->
    <model name="ground_plane">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>10 10</size>
            </plane>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>10 10</size>
            </plane>
          </geometry>
          <material>
            <ambient>0.8 0.8 0.8 1</ambient>
            <diffuse>0.8 0.8 0.8 1</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <!-- Table for objects -->
    <model name="table">
      <static>true</static>
      <pose>0.5 0 0.25 0 0 0</pose>
      <link name="link">
        <collision name="surface">
          <geometry>
            <box>
              <size>0.6 0.4 0.02</size>
            </box>
          </geometry>
        </collision>
        <visual name="surface">
          <geometry>
            <box>
              <size>0.6 0.4 0.02</size>
            </box>
          </geometry>
          <material>
            <ambient>0.6 0.4 0.2 1</ambient>
            <diffuse>0.6 0.4 0.2 1</diffuse>
          </material>
        </visual>
        <!-- Table legs -->
        <collision name="leg1">
          <pose>0.25 0.15 -0.125 0 0 0</pose>
          <geometry><box><size>0.04 0.04 0.25</size></box></geometry>
        </collision>
        <visual name="leg1">
          <pose>0.25 0.15 -0.125 0 0 0</pose>
          <geometry><box><size>0.04 0.04 0.25</size></box></geometry>
          <material><ambient>0.4 0.3 0.2 1</ambient><diffuse>0.4 0.3 0.2 1</diffuse></material>
        </visual>
        <collision name="leg2">
          <pose>-0.25 0.15 -0.125 0 0 0</pose>
          <geometry><box><size>0.04 0.04 0.25</size></box></geometry>
        </collision>
        <visual name="leg2">
          <pose>-0.25 0.15 -0.125 0 0 0</pose>
          <geometry><box><size>0.04 0.04 0.25</size></box></geometry>
          <material><ambient>0.4 0.3 0.2 1</ambient><diffuse>0.4 0.3 0.2 1</diffuse></material>
        </visual>
        <collision name="leg3">
          <pose>0.25 -0.15 -0.125 0 0 0</pose>
          <geometry><box><size>0.04 0.04 0.25</size></box></geometry>
        </collision>
        <visual name="leg3">
          <pose>0.25 -0.15 -0.125 0 0 0</pose>
          <geometry><box><size>0.04 0.04 0.25</size></box></geometry>
          <material><ambient>0.4 0.3 0.2 1</ambient><diffuse>0.4 0.3 0.2 1</diffuse></material>
        </visual>
        <collision name="leg4">
          <pose>-0.25 -0.15 -0.125 0 0 0</pose>
          <geometry><box><size>0.04 0.04 0.25</size></box></geometry>
        </collision>
        <visual name="leg4">
          <pose>-0.25 -0.15 -0.125 0 0 0</pose>
          <geometry><box><size>0.04 0.04 0.25</size></box></geometry>
          <material><ambient>0.4 0.3 0.2 1</ambient><diffuse>0.4 0.3 0.2 1</diffuse></material>
        </visual>
      </link>
    </model>

    <!-- Red cube (target object) -->
    <model name="red_cube">
      <pose>0.4 0.1 0.28 0 0 0</pose>
      <link name="link">
        <inertial>
          <mass>0.05</mass>
          <inertia>
            <ixx>0.0001</ixx>
            <iyy>0.0001</iyy>
            <izz>0.0001</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry>
            <box>
              <size>0.04 0.04 0.04</size>
            </box>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>0.04 0.04 0.04</size>
            </box>
          </geometry>
          <material>
            <ambient>1 0 0 1</ambient>
            <diffuse>1 0 0 1</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <!-- Blue cube -->
    <model name="blue_cube">
      <pose>0.5 -0.1 0.28 0 0 0</pose>
      <link name="link">
        <inertial>
          <mass>0.05</mass>
          <inertia>
            <ixx>0.0001</ixx>
            <iyy>0.0001</iyy>
            <izz>0.0001</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry>
            <box>
              <size>0.04 0.04 0.04</size>
            </box>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>0.04 0.04 0.04</size>
            </box>
          </geometry>
          <material>
            <ambient>0 0 1 1</ambient>
            <diffuse>0 0 1 1</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <!-- Green cube -->
    <model name="green_cube">
      <pose>0.6 0.05 0.28 0 0 0</pose>
      <link name="link">
        <inertial>
          <mass>0.05</mass>
          <inertia>
            <ixx>0.0001</ixx>
            <iyy>0.0001</iyy>
            <izz>0.0001</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry>
            <box>
              <size>0.04 0.04 0.04</size>
            </box>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>0.04 0.04 0.04</size>
            </box>
          </geometry>
          <material>
            <ambient>0 1 0 1</ambient>
            <diffuse>0 1 0 1</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <!-- L-ONE Robotic Arm (4 DOF + Gripper) -->
    <!-- Based on real L-ONE CyberBrick arm: base rotation + shoulder + elbow + gripper -->
    <model name="l_one_arm">
      <static>false</static>
      <pose>0.5 0 0.26 0 0 0</pose>
      
      <!-- Base pedestal (fixed to table) -->
      <link name="base_pedestal">
        <inertial>
          <mass>0.3</mass>
          <inertia><ixx>0.0002</ixx><iyy>0.0002</iyy><izz>0.0002</izz></inertia>
        </inertial>
        <collision name="collision">
          <geometry><cylinder><radius>0.04</radius><length>0.03</length></cylinder></geometry>
        </collision>
        <visual name="visual">
          <geometry><cylinder><radius>0.04</radius><length>0.03</length></cylinder></geometry>
          <material><ambient>0.9 0.9 0.9 1</ambient><diffuse>0.9 0.9 0.9 1</diffuse></material>
        </visual>
      </link>

      <!-- Turntable (rotates on base) -->
      <link name="turntable">
        <pose relative_to="base_pedestal">0 0 0.025 0 0 0</pose>
        <inertial>
          <mass>0.15</mass>
          <inertia><ixx>0.0001</ixx><iyy>0.0001</iyy><izz>0.0001</izz></inertia>
        </inertial>
        <collision name="collision">
          <geometry><cylinder><radius>0.035</radius><length>0.02</length></cylinder></geometry>
        </collision>
        <visual name="visual">
          <geometry><cylinder><radius>0.035</radius><length>0.02</length></cylinder></geometry>
          <material><ambient>0.85 0.85 0.85 1</ambient><diffuse>0.85 0.85 0.85 1</diffuse></material>
        </visual>
      </link>

      <!-- Lower arm (shoulder joint) -->
      <link name="lower_arm">
        <pose relative_to="turntable">0 0 0.02 0 0 0</pose>
        <inertial>
          <mass>0.1</mass>
          <inertia><ixx>0.0001</ixx><iyy>0.0001</iyy><izz>0.0001</izz></inertia>
        </inertial>
        <collision name="collision">
          <pose>0 0 0.04 0 0 0</pose>
          <geometry><box><size>0.03 0.025 0.08</size></box></geometry>
        </collision>
        <visual name="visual">
          <pose>0 0 0.04 0 0 0</pose>
          <geometry><box><size>0.03 0.025 0.08</size></box></geometry>
          <material><ambient>0.9 0.9 0.9 1</ambient><diffuse>0.9 0.9 0.9 1</diffuse></material>
        </visual>
      </link>

      <!-- Upper arm (elbow joint) -->
      <link name="upper_arm">
        <pose relative_to="lower_arm">0 0 0.08 0 0 0</pose>
        <inertial>
          <mass>0.08</mass>
          <inertia><ixx>0.00008</ixx><iyy>0.00008</iyy><izz>0.00008</izz></inertia>
        </inertial>
        <collision name="collision">
          <pose>0 0 0.035 0 0 0</pose>
          <geometry><box><size>0.025 0.02 0.07</size></box></geometry>
        </collision>
        <visual name="visual">
          <pose>0 0 0.035 0 0 0</pose>
          <geometry><box><size>0.025 0.02 0.07</size></box></geometry>
          <material><ambient>0.9 0.9 0.9 1</ambient><diffuse>0.9 0.9 0.9 1</diffuse></material>
        </visual>
      </link>

      <!-- Gripper base -->
      <link name="gripper_base">
        <pose relative_to="upper_arm">0 0 0.07 0 0 0</pose>
        <inertial>
          <mass>0.03</mass>
          <inertia><ixx>0.00002</ixx><iyy>0.00002</iyy><izz>0.00002</izz></inertia>
        </inertial>
        <collision name="collision">
          <geometry><box><size>0.02 0.04 0.015</size></box></geometry>
        </collision>
        <visual name="visual">
          <geometry><box><size>0.02 0.04 0.015</size></box></geometry>
          <material><ambient>0.3 0.3 0.3 1</ambient><diffuse>0.3 0.3 0.3 1</diffuse></material>
        </visual>
      </link>

      <!-- Left gripper finger -->
      <link name="gripper_left">
        <pose relative_to="gripper_base">0 0.015 0.015 0 0 0</pose>
        <inertial>
          <mass>0.01</mass>
          <inertia><ixx>0.000001</ixx><iyy>0.000001</iyy><izz>0.000001</izz></inertia>
        </inertial>
        <collision name="collision">
          <pose>0 0 0.012 0 0 0</pose>
          <geometry><box><size>0.008 0.005 0.025</size></box></geometry>
        </collision>
        <visual name="visual">
          <pose>0 0 0.012 0 0 0</pose>
          <geometry><box><size>0.008 0.005 0.025</size></box></geometry>
          <material><ambient>0.2 0.2 0.2 1</ambient><diffuse>0.2 0.2 0.2 1</diffuse></material>
        </visual>
      </link>

      <!-- Right gripper finger -->
      <link name="gripper_right">
        <pose relative_to="gripper_base">0 -0.015 0.015 0 0 0</pose>
        <inertial>
          <mass>0.01</mass>
          <inertia><ixx>0.000001</ixx><iyy>0.000001</iyy><izz>0.000001</izz></inertia>
        </inertial>
        <collision name="collision">
          <pose>0 0 0.012 0 0 0</pose>
          <geometry><box><size>0.008 0.005 0.025</size></box></geometry>
        </collision>
        <visual name="visual">
          <pose>0 0 0.012 0 0 0</pose>
          <geometry><box><size>0.008 0.005 0.025</size></box></geometry>
          <material><ambient>0.2 0.2 0.2 1</ambient><diffuse>0.2 0.2 0.2 1</diffuse></material>
        </visual>
      </link>

      <!-- Fix base to world (on table) -->
      <joint name="arm_fixed" type="fixed">
        <parent>world</parent>
        <child>base_pedestal</child>
      </joint>

      <!-- Joint 1: Base rotation (servo 180°) -->
      <joint name="base_rotation" type="revolute">
        <parent>base_pedestal</parent>
        <child>turntable</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-1.57</lower>
            <upper>1.57</upper>
            <velocity>1.0</velocity>
            <effort>10</effort>
          </limit>
          <dynamics><damping>2.0</damping><friction>0.5</friction></dynamics>
        </axis>
      </joint>

      <!-- Joint 2: Shoulder (servo 360° velocity with worm gear) -->
      <joint name="shoulder" type="revolute">
        <parent>turntable</parent>
        <child>lower_arm</child>
        <axis>
          <xyz>0 1 0</xyz>
          <limit>
            <lower>-0.5</lower>
            <upper>1.57</upper>
            <velocity>0.8</velocity>
            <effort>10</effort>
          </limit>
          <dynamics><damping>2.0</damping><friction>0.5</friction></dynamics>
        </axis>
      </joint>

      <!-- Joint 3: Elbow (servo 360° velocity with worm gear) -->
      <joint name="elbow" type="revolute">
        <parent>lower_arm</parent>
        <child>upper_arm</child>
        <axis>
          <xyz>0 1 0</xyz>
          <limit>
            <lower>-1.57</lower>
            <upper>1.57</upper>
            <velocity>0.8</velocity>
            <effort>10</effort>
          </limit>
          <dynamics><damping>2.0</damping><friction>0.5</friction></dynamics>
        </axis>
      </joint>

      <!-- Joint 4: Gripper mount (fixed) -->
      <joint name="gripper_mount" type="fixed">
        <parent>upper_arm</parent>
        <child>gripper_base</child>
      </joint>

      <!-- Joint 5: Left gripper finger (prismatic) -->
      <joint name="gripper_left_joint" type="prismatic">
        <parent>gripper_base</parent>
        <child>gripper_left</child>
        <axis>
          <xyz>0 1 0</xyz>
          <limit><lower>-0.01</lower><upper>0.005</upper></limit>
          <dynamics><damping>0.1</damping></dynamics>
        </axis>
      </joint>

      <!-- Joint 6: Right gripper finger (prismatic, mirrored) -->
      <joint name="gripper_right_joint" type="prismatic">
        <parent>gripper_base</parent>
        <child>gripper_right</child>
        <axis>
          <xyz>0 -1 0</xyz>
          <limit><lower>-0.01</lower><upper>0.005</upper></limit>
          <dynamics><damping>0.1</damping></dynamics>
        </axis>
      </joint>

      <!-- Joint Position Controllers -->
      <!-- Base rotation (position control - servo 180°) -->
      <plugin filename="libignition-gazebo-joint-position-controller-system.so" 
              name="ignition::gazebo::systems::JointPositionController">
        <joint_name>base_rotation</joint_name>
        <topic>/arm/base</topic>
        <p_gain>5</p_gain>
        <i_gain>0.01</i_gain>
        <d_gain>2.0</d_gain>
      </plugin>
      
      <!-- Shoulder (position control) - higher gains to overcome gravity -->
      <plugin filename="libignition-gazebo-joint-position-controller-system.so" 
              name="ignition::gazebo::systems::JointPositionController">
        <joint_name>shoulder</joint_name>
        <topic>/arm/shoulder</topic>
        <p_gain>50</p_gain>
        <i_gain>5.0</i_gain>
        <d_gain>10.0</d_gain>
      </plugin>
      
      <!-- Elbow (position control) -->
      <plugin filename="libignition-gazebo-joint-position-controller-system.so" 
              name="ignition::gazebo::systems::JointPositionController">
        <joint_name>elbow</joint_name>
        <topic>/arm/elbow</topic>
        <p_gain>6</p_gain>
        <i_gain>0.01</i_gain>
        <d_gain>2.5</d_gain>
      </plugin>
      
      <!-- Gripper (position control - both fingers together) -->
      <plugin filename="libignition-gazebo-joint-position-controller-system.so" 
              name="ignition::gazebo::systems::JointPositionController">
        <joint_name>gripper_left_joint</joint_name>
        <topic>/arm/gripper</topic>
        <p_gain>20</p_gain>
        <i_gain>0.01</i_gain>
        <d_gain>1.0</d_gain>
      </plugin>
      <plugin filename="libignition-gazebo-joint-position-controller-system.so" 
              name="ignition::gazebo::systems::JointPositionController">
        <joint_name>gripper_right_joint</joint_name>
        <topic>/arm/gripper</topic>
        <p_gain>20</p_gain>
        <i_gain>0.01</i_gain>
        <d_gain>1.0</d_gain>
      </plugin>

    </model>

    <!-- CyberBrick Truck (Ackermann Steering) -->
    <!-- Based on real CyberBrick truck: rear drive + front steering -->
    <model name="cyberbrick_truck">
      <pose>-0.5 0 0.035 0 0 0</pose>
      
      <!-- Chassis -->
      <link name="chassis">
        <inertial>
          <mass>0.8</mass>
          <inertia>
            <ixx>0.002</ixx><ixy>0</ixy><ixz>0</ixz>
            <iyy>0.004</iyy><iyz>0</iyz>
            <izz>0.004</izz>
          </inertia>
        </inertial>
        <!-- Main body -->
        <collision name="body_collision">
          <pose>0 0 0.02 0 0 0</pose>
          <geometry><box><size>0.20 0.10 0.04</size></box></geometry>
        </collision>
        <visual name="body_visual">
          <pose>0 0 0.02 0 0 0</pose>
          <geometry><box><size>0.20 0.10 0.04</size></box></geometry>
          <material><ambient>0.9 0.8 0.2 1</ambient><diffuse>0.9 0.8 0.2 1</diffuse></material>
        </visual>
        <!-- Cabin -->
        <collision name="cabin_collision">
          <pose>-0.05 0 0.06 0 0 0</pose>
          <geometry><box><size>0.08 0.08 0.04</size></box></geometry>
        </collision>
        <visual name="cabin_visual">
          <pose>-0.05 0 0.06 0 0 0</pose>
          <geometry><box><size>0.08 0.08 0.04</size></box></geometry>
          <material><ambient>0.9 0.8 0.2 1</ambient><diffuse>0.9 0.8 0.2 1</diffuse></material>
        </visual>
      </link>

      <!-- Rear left wheel (driven) -->
      <link name="rear_left_wheel">
        <pose relative_to="chassis">-0.07 0.055 0 -1.5707 0 0</pose>
        <inertial>
          <mass>0.08</mass>
          <inertia><ixx>0.00005</ixx><iyy>0.00005</iyy><izz>0.00008</izz></inertia>
        </inertial>
        <collision name="collision">
          <geometry><cylinder><radius>0.035</radius><length>0.02</length></cylinder></geometry>
          <surface>
            <friction><ode><mu>1.0</mu><mu2>1.0</mu2></ode></friction>
          </surface>
        </collision>
        <visual name="visual">
          <geometry><cylinder><radius>0.035</radius><length>0.02</length></cylinder></geometry>
          <material><ambient>0.15 0.15 0.15 1</ambient><diffuse>0.15 0.15 0.15 1</diffuse></material>
        </visual>
      </link>

      <!-- Rear right wheel (driven) -->
      <link name="rear_right_wheel">
        <pose relative_to="chassis">-0.07 -0.055 0 -1.5707 0 0</pose>
        <inertial>
          <mass>0.08</mass>
          <inertia><ixx>0.00005</ixx><iyy>0.00005</iyy><izz>0.00008</izz></inertia>
        </inertial>
        <collision name="collision">
          <geometry><cylinder><radius>0.035</radius><length>0.02</length></cylinder></geometry>
          <surface>
            <friction><ode><mu>1.0</mu><mu2>1.0</mu2></ode></friction>
          </surface>
        </collision>
        <visual name="visual">
          <geometry><cylinder><radius>0.035</radius><length>0.02</length></cylinder></geometry>
          <material><ambient>0.15 0.15 0.15 1</ambient><diffuse>0.15 0.15 0.15 1</diffuse></material>
        </visual>
      </link>

      <!-- Front left steering knuckle -->
      <link name="front_left_knuckle">
        <pose relative_to="chassis">0.07 0.045 0 0 0 0</pose>
        <inertial>
          <mass>0.02</mass>
          <inertia><ixx>0.000001</ixx><iyy>0.000001</iyy><izz>0.000001</izz></inertia>
        </inertial>
        <visual name="visual">
          <geometry><cylinder><radius>0.005</radius><length>0.01</length></cylinder></geometry>
          <material><ambient>0.3 0.3 0.3 1</ambient><diffuse>0.3 0.3 0.3 1</diffuse></material>
        </visual>
      </link>

      <!-- Front left wheel -->
      <link name="front_left_wheel">
        <pose relative_to="front_left_knuckle">0 0.01 0 -1.5707 0 0</pose>
        <inertial>
          <mass>0.06</mass>
          <inertia><ixx>0.00003</ixx><iyy>0.00003</iyy><izz>0.00005</izz></inertia>
        </inertial>
        <collision name="collision">
          <geometry><cylinder><radius>0.035</radius><length>0.02</length></cylinder></geometry>
          <surface>
            <friction><ode><mu>1.0</mu><mu2>1.0</mu2></ode></friction>
          </surface>
        </collision>
        <visual name="visual">
          <geometry><cylinder><radius>0.035</radius><length>0.02</length></cylinder></geometry>
          <material><ambient>0.15 0.15 0.15 1</ambient><diffuse>0.15 0.15 0.15 1</diffuse></material>
        </visual>
      </link>

      <!-- Front right steering knuckle -->
      <link name="front_right_knuckle">
        <pose relative_to="chassis">0.07 -0.045 0 0 0 0</pose>
        <inertial>
          <mass>0.02</mass>
          <inertia><ixx>0.000001</ixx><iyy>0.000001</iyy><izz>0.000001</izz></inertia>
        </inertial>
        <visual name="visual">
          <geometry><cylinder><radius>0.005</radius><length>0.01</length></cylinder></geometry>
          <material><ambient>0.3 0.3 0.3 1</ambient><diffuse>0.3 0.3 0.3 1</diffuse></material>
        </visual>
      </link>

      <!-- Front right wheel -->
      <link name="front_right_wheel">
        <pose relative_to="front_right_knuckle">0 -0.01 0 -1.5707 0 0</pose>
        <inertial>
          <mass>0.06</mass>
          <inertia><ixx>0.00003</ixx><iyy>0.00003</iyy><izz>0.00005</izz></inertia>
        </inertial>
        <collision name="collision">
          <geometry><cylinder><radius>0.035</radius><length>0.02</length></cylinder></geometry>
          <surface>
            <friction><ode><mu>1.0</mu><mu2>1.0</mu2></ode></friction>
          </surface>
        </collision>
        <visual name="visual">
          <geometry><cylinder><radius>0.035</radius><length>0.02</length></cylinder></geometry>
          <material><ambient>0.15 0.15 0.15 1</ambient><diffuse>0.15 0.15 0.15 1</diffuse></material>
        </visual>
      </link>

      <!-- Camera link -->
      <link name="camera_link">
        <pose relative_to="chassis">0.08 0 0.06 0 0 0</pose>
        <inertial>
          <mass>0.02</mass>
          <inertia><ixx>0.00001</ixx><iyy>0.00001</iyy><izz>0.00001</izz></inertia>
        </inertial>
        <collision name="collision">
          <geometry><box><size>0.02 0.03 0.02</size></box></geometry>
        </collision>
        <visual name="visual">
          <geometry><box><size>0.02 0.03 0.02</size></box></geometry>
          <material><ambient>0.1 0.1 0.1 1</ambient><diffuse>0.1 0.1 0.1 1</diffuse></material>
        </visual>
        <sensor name="camera" type="camera">
          <always_on>true</always_on>
          <update_rate>30</update_rate>
          <camera>
            <horizontal_fov>1.047</horizontal_fov>
            <image>
              <width>640</width>
              <height>480</height>
              <format>R8G8B8</format>
            </image>
            <clip>
              <near>0.01</near>
              <far>10</far>
            </clip>
          </camera>
          <topic>/camera/image_raw</topic>
        </sensor>
      </link>

      <!-- JOINTS -->
      
      <!-- Rear wheel joints (driven) -->
      <joint name="rear_left_wheel_joint" type="revolute">
        <parent>chassis</parent>
        <child>rear_left_wheel</child>
        <axis>
          <xyz>0 0 1</xyz>
          <dynamics><damping>0.1</damping><friction>0.1</friction></dynamics>
        </axis>
      </joint>

      <joint name="rear_right_wheel_joint" type="revolute">
        <parent>chassis</parent>
        <child>rear_right_wheel</child>
        <axis>
          <xyz>0 0 1</xyz>
          <dynamics><damping>0.1</damping><friction>0.1</friction></dynamics>
        </axis>
      </joint>

      <!-- Front steering joints -->
      <joint name="front_left_steering_joint" type="revolute">
        <parent>chassis</parent>
        <child>front_left_knuckle</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit><lower>-0.6</lower><upper>0.6</upper></limit>
          <dynamics><damping>0.5</damping><friction>0.1</friction></dynamics>
        </axis>
      </joint>

      <joint name="front_right_steering_joint" type="revolute">
        <parent>chassis</parent>
        <child>front_right_knuckle</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit><lower>-0.6</lower><upper>0.6</upper></limit>
          <dynamics><damping>0.5</damping><friction>0.1</friction></dynamics>
        </axis>
      </joint>

      <!-- Front wheel spin joints -->
      <joint name="front_left_wheel_joint" type="revolute">
        <parent>front_left_knuckle</parent>
        <child>front_left_wheel</child>
        <axis>
          <xyz>0 0 1</xyz>
          <dynamics><damping>0.01</damping></dynamics>
        </axis>
      </joint>

      <joint name="front_right_wheel_joint" type="revolute">
        <parent>front_right_knuckle</parent>
        <child>front_right_wheel</child>
        <axis>
          <xyz>0 0 1</xyz>
          <dynamics><damping>0.01</damping></dynamics>
        </axis>
      </joint>

      <!-- Camera joint -->
      <joint name="camera_joint" type="fixed">
        <parent>chassis</parent>
        <child>camera_link</child>
      </joint>

      <!-- PLUGINS -->
      
      <!-- Ackermann steering plugin -->
      <plugin filename="libignition-gazebo-ackermann-steering-system.so" 
              name="ignition::gazebo::systems::AckermannSteering">
        <left_joint>front_left_wheel_joint</left_joint>
        <right_joint>front_right_wheel_joint</right_joint>
        <left_steering_joint>front_left_steering_joint</left_steering_joint>
        <right_steering_joint>front_right_steering_joint</right_steering_joint>
        <wheel_separation>0.09</wheel_separation>
        <kingpin_width>0.09</kingpin_width>
        <wheel_base>0.14</wheel_base>
        <wheel_radius>0.035</wheel_radius>
        <steering_limit>0.5</steering_limit>
        <topic>/truck/cmd_vel</topic>
        <odom_topic>/truck/odom</odom_topic>
        <frame_id>odom</frame_id>
        <child_frame_id>chassis</child_frame_id>
      </plugin>

      <!-- Rear wheel drive (velocity control) -->
      <plugin filename="libignition-gazebo-joint-controller-system.so"
              name="ignition::gazebo::systems::JointController">
        <joint_name>rear_left_wheel_joint</joint_name>
        <topic>/truck/rear_left_wheel_vel</topic>
        <initial_velocity>0</initial_velocity>
      </plugin>
      <plugin filename="libignition-gazebo-joint-controller-system.so"
              name="ignition::gazebo::systems::JointController">
        <joint_name>rear_right_wheel_joint</joint_name>
        <topic>/truck/rear_right_wheel_vel</topic>
        <initial_velocity>0</initial_velocity>
      </plugin>

    </model>

    <!-- Overhead Camera (fixed, looking down at the table) -->
    <model name="overhead_camera">
      <static>true</static>
      <pose>0.5 0 0.8 3.14159 1.5707963 0</pose>  <!-- Above table, roll 180° + pitch 90° to look down at table -->
      <link name="link">
        <inertial>
          <mass>0.1</mass>
          <inertia>
            <ixx>0.0001</ixx>
            <iyy>0.0001</iyy>
            <izz>0.0001</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry>
            <box>
              <size>0.05 0.05 0.05</size>
            </box>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>0.05 0.05 0.05</size>
            </box>
          </geometry>
          <material>
            <ambient>0.2 0.2 0.2 1</ambient>
            <diffuse>0.2 0.2 0.2 1</diffuse>
          </material>
        </visual>
        <sensor name="overhead_camera" type="camera">
          <always_on>true</always_on>
          <update_rate>10</update_rate>
          <camera>
            <horizontal_fov>1.2</horizontal_fov>
            <image>
              <width>640</width>
              <height>480</height>
              <format>R8G8B8</format>
            </image>
            <clip>
              <near>0.1</near>
              <far>10</far>
            </clip>
          </camera>
          <topic>/overhead_camera/image_raw</topic>
        </sensor>
      </link>
    </model>

  </world>
</sdf>
